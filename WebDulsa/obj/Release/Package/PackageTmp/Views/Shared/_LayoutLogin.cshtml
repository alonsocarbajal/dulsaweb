<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <link rel="stylesheet" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/vendor/font-awesome/css/font-awesome.min.css" />
    @Styles.Render("~/login/css")
</head>
<body class="hold-transition sidebar-mini">
    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                @*<div class="login100-form-title" style=" height:100px; background-color:#007bff;">*@
                <div class="login100-form-title" style=" height:100px; background-color:#001139;">
                    <span class="login100-form-title-1">
                        @ViewBag.Title
                    </span>
                </div>
                @*<img src="~/Content/img/logo.png" style="float:right;">
        <img src="~/Content/img/logo2.png" style="float:left;">*@
                @RenderBody()
            </div>
    </div>
</div>
               
                @Scripts.Render("~/login/js")
                @RenderSection("scripts", required: false)
</body>
</html>
